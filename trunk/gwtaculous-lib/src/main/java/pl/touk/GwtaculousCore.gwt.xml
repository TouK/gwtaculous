<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.1.1/distro-source/core/src/gwt-module.dtd">
	
<module>
	<!-- automaticly adds dependant java scripts to host page -->
    <!--<script src="js/prototype.js" />-->
    <!--<script src="js/scriptaculous.js?load=effects" />-->
    <inherits name="com.google.gwt.logging.Logging"/>

	<source path="gwtaculous" />

    <!-- logging configuration -->
    <set-property name="gwt.logging.enabled" value="TRUE"/>
    <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>

    <entry-point class="pl.touk.gwtaculous.core.GwtaculousCoreEntryPoint"/>

</module>        